<% layout('layout/boilerplate') %>
    
        <div class="pcoded-wrapper">
            <div class="pcoded-content">
                <div class="pcoded-inner-content">

                    <div class="main-body">
                        <div class="page-wrapper">
                            <!-- [ Main Content ] start -->
                            <div class="row">
                                <div class="col-xl-6 col-md-6 mx-auto">
                                    <div class="card Recent-Users">
                                        <div class="card-header">
                                            <h5>Benefit Plan </h5>
                                        </div>
                                        <div class="row">
                                            <div class="col-sm">Benefit Plan ID</div>
                                            <div class="col-sm">
                                                <%=benefitPlan['benefitPlanID']%>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-sm">Plan Name</div>
                                            <div class="col-sm">
                                                <%=benefitPlan['planName']%>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-sm">Deductable</div>
                                            <div class="col-sm">
                                                <%=benefitPlan['deductable']%>
                                            </div>
                                        </div>
                                        <div class="row">
                                            <div class="col-sm">Percentage CoPay</div>
                                            <div class="col-sm">
                                                <%=benefitPlan['percentageCoPay']%>
                                            </div>
                                        </div>
                                    </div>
                                    <a class="card-link btn btn-outline-info"
                                        href="/benefit-plan/<%=benefitPlan['benefitPlanID']%>/edit">Edit</a>
                                    <form class="d-inline"
                                        action="/benefit-plan/<%=benefitPlan['benefitPlanID']%>?_method=DELETE"
                                        method="POST">
                                        <button class="btn btn-outline-danger">Delete</button>
                                    </form>
                                    <a class="card-link btn btn-outline-info" href="/benefit-plan">Back to list</a>

                                </div>
                            </div>
                            <!-- [ Main Content ] end -->
                        </div>
                    </div>
                </div>
            </div>
        </div>
   